{
  "name": "gatsby-starter-theme-workspaces",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "Jan Grzegorowski",
  "license": "MIT",
  "scripts": {
    "theme": "yarn workspace gatsby-ghost-theme",
    "blog": "yarn workspace blog",
    "serve": "serve blog/public/",
    "build:dev": "CONFIG_ENV=development yarn blog build",
    "build:stag": "CONFIG_ENV=staging yarn blog build",
    "build:prod": "CONFIG_ENV=production yarn blog build",
    "clean": "yarn blog clean",
    "upgrade": "ncu --packageFile blog/package.json"
  },
  "workspaces": [
    "gatsby-ghost-theme",
    "blog"
  ],
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
