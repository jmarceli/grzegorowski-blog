{
  "name": "gatsby-starter-theme-workspaces",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "author": "Jan Grzegorowski",
  "license": "MIT",
  "scripts": {
    "theme": "yarn workspace gatsby-ghost-theme",
    "blog": "yarn workspace blog",
    "develop": "yarn blog develop",
    "serve": "serve blog/public/",
    "build:prod": "CONFIG_ENV=production yarn blog build",
    "clean": "yarn blog clean",
    "upgrade": "ncu --packageFile blog/package.json",
    "publish": "yarn build:prod && yarn gh-pages --dist blog/public --dotfiles --repo https://github.com/jmarceli/grzegorowski-blog.git"
  },
  "workspaces": [
    "gatsby-ghost-theme",
    "blog"
  ],
  "devDependencies": {
    "prettier": "^1.18.2"
  }
}
